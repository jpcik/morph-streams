    </owl:ObjectProperty>
    


    <!-- file:/home/aurona/0AlleWerk/Navorsing/Ontologies/NAP/NAP#isAssistedBy -->

    <owl:ObjectProperty rdf:about="&NAP;isAssistedBy"/>
    


    <!-- file:/home/aurona/0AlleWerk/Navorsing/Ontologies/NAP/NAP#requiresAbility -->

    <owl:ObjectProperty rdf:about="&NAP;requiresAbility"/>
    


    <!-- http://ksg.meraka.co.za/adolena.owl#affects -->

