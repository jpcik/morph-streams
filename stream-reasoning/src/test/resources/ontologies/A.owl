    <!-- http://ksg.meraka.co.za/adolena.owl#assistsWith -->

    <owl:ObjectProperty rdf:about="http://ksg.meraka.co.za/adolena.owl#assistsWith">
        <rdfs:comment rdf:datatype="&xsd;string">MK: the &quot;assistsWith&quot; in the sense of that some person has a disability and then the device assists with realizing some ability</rdfs:comment>
        <owl:inverseOf rdf:resource="&NAP;isAssistedBy"/>
    </owl:ObjectProperty>
    


